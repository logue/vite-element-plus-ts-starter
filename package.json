{
  "name": "vite-elemnt-plus-ts-starter",
  "version": "1.7.0",
  "description": "Vue3 Element Plus TypeScript Startar project for Vite.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-elemnt-plus-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-elemnt-plus-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-elemnt-plus-ts-starter/issues"
  },
  "engines": {
    "node": ">=18.12.5",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "preview": "vite preview",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "prepare": "husky install"
  },
  "dependencies": {
    "element-plus": "^2.4.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.11",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@iconify-json/ep": "^1.1.13",
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node-lts": "^18.12.5",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/reset": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-config-standard-with-typescript": "^40.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-vue": "latest",
    "eslint-plugin-vuejs-accessibility": "^2.2.0",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.10.0",
    "sass": "^1.69.5",
    "stylelint": "^16.0.1",
    "stylelint-config-recommended-scss": "^13.1.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.3",
    "typescript": "*",
    "unocss": "^0.58.0",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.7",
    "vite-plugin-checker": "^0.6.2",
    "vitest": "^1.0.2",
    "vue-tsc": "^1.8.25"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "json5": "^2.2.3",
    "postcss": "^8.4.31",
    "semver": "^7.5.3",
    "yaml": "^2.3.2"
  },
  "stackblitz": {
    "startCommand": "yarn run test:unit"
  }
}
