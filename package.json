{
  "name": "vite-elemnt-plus-ts-starter",
  "version": "1.10.6",
  "description": "Vue3 Element Plus TypeScript Startar project for Vite.",
  "license": "MIT",
  "type": "module",
  "private": true,
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/vite-elemnt-plus-ts-starter",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/vite-elemnt-plus-ts-starter.git"
  },
  "bugs": {
    "url": "https://github.com/logue/vite-elemnt-plus-ts-starter/issues"
  },
  "engines": {
    "node": ">=20.12.7",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.3.1",
  "scripts": {
    "dev": "vite",
    "clean": "rimraf ./node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:analyze": "vite build --mode analyze",
    "build:clean": "rimraf dist",
    "lint": "eslint . --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . -w -u",
    "lint:style": "stylelint \"./**/*.{css,sass,scss,htm,html,vue}\" --fix --cache-location ./node_modules/.vite/vite-plugin-stylelint && prettier \"./**/*.{css,sass,scss,htm,html,vue}\" -w -u",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:coverage": "vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "prepare": "husky install"
  },
  "dependencies": {
    "element-plus": "^2.7.6",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.31",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.6.0",
    "@iconify-json/ep": "^1.1.15",
    "@rushstack/eslint-patch": "^1.10.3",
    "@tsconfig/node-lts": "^20.1.3",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.10",
    "@unocss/eslint-config": "^0.61.3",
    "@unocss/reset": "^0.61.3",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitest/coverage-v8": "^1.6.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-tsdoc": "^0.3.0",
    "eslint-plugin-vue": "^9.27.0",
    "eslint-plugin-vuejs-accessibility": "^2.3.1",
    "eslint-plugin-yaml": "^1.0.3",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "npm-run-all2": "^6.2.2",
    "postcss-html": "^1.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.8",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.77.6",
    "stylelint": "^16.6.1",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.15.0",
    "unocss": "^0.61.3",
    "unplugin-element-plus": "^0.8.0",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.3.3",
    "vite-plugin-checker": "^0.7.1",
    "vitest": "^1.6.0",
    "vue-tsc": "^2.0.26"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,yml,yaml,vue,htm,html,md}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*.{css,sass,scss,vue,htm,html}": "stylelint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-stylelint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "get-func-name": ">=2.0.2",
    "ip": ">=2.0.1",
    "json5": ">=2.2.3",
    "lodash": ">=4.17.21",
    "postcss": ">=8.4.31",
    "semver": ">=7.5.3",
    "tar": ">=6.2.1",
    "vite": ">=5.0.12",
    "yaml": ">=2.3.2"
  },
  "stackblitz": {
    "startCommand": "yarn run test:unit"
  }
}
